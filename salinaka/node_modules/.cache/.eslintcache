web-dev-portfolio/salinaka/src/component/shopmodal/ShopModal.jsx",[],[],"/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/productCard/ProductPanel1.jsx",[],[],"/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/productCard/ProductCard2.jsx",["156"],[],"import React, { useState, useContext, useEffect } from \"react\";\nimport Card from '@mui/material/Card';\nimport CardContent from '@mui/material/CardContent';\nimport CardMedia from '@mui/material/CardMedia';\nimport Typography from '@mui/material/Typography';\nimport { Check } from '@phosphor-icons/react';\nimport { useNavigate } from \"react-router-dom\";\nimport \"./sass/productcard.css\";\nimport MainContext from '../context/Context';\n\nexport default function ProductCard2({id}) {\n    // get 'products' state and 'local' state\n    const { products, setProducts, cartLocal, setCartLocal } = useContext(MainContext);\n\n    const [item, setItem] = useState(products.filter(product => product.id === Number(id))[0]);\n\n    useEffect(()=>{\n        setItem(products.filter(product => product.id === Number(id))[0])\n    }, [products])\n   \n    const updateItem = () => {\n        let cart = JSON.parse(localStorage.getItem('cart')) || [];\n\n        if (!item.isAdded) {\n            const selectedSize = item.size[0];\n            const selectedColor = item.color ? item.color[0] : '';\n            const selectedCombo = item.id + '-' + selectedSize + '-' + selectedColor;\n            const itemToAdd = { comboId: selectedCombo, quantity: 1, productId: item.id, name: item.name, price: item.price, img: item.img, size: selectedSize, color: selectedColor };\n            // update isAdded to true\n            setProducts(prevProducts => {\n                const updatedProducts = [];\n                prevProducts.forEach(product => {\n                    if (product.id !== item.id) {\n                        updatedProducts.push(product)\n                    }\n                    else {\n                        updatedProducts.push({ ...product, isAdded: true })\n                    }\n                })\n                return updatedProducts\n            })\n            cart.push(itemToAdd);\n        } else {\n            // if product has been added before, remove it from cart\n            const updatedCart = []\n            cart.forEach(cartItem => {\n                if (cartItem.productId !== item.id) {\n                    updatedCart.push(cartItem);\n                }\n            })\n            cart = updatedCart;\n            // update isAdded to false\n            setProducts(prevProducts => {\n                const updatedProducts = [];\n                prevProducts.forEach(product => {\n                    if (product.id !== item.id) {\n                        updatedProducts.push(product)\n                    }\n                    else {\n                        updatedProducts.push({ ...product, isAdded: false })\n                    }\n                })\n                return updatedProducts\n            })\n        }\n        localStorage.setItem('cart', JSON.stringify(cart));\n        setCartLocal(!cartLocal);\n    }\n\n    const btnText = item.isAdded ? \"Remove from basket\" : \"Add to basket\";\n    const btnStyle = item.isAdded ? { backgroundColor: \"rgb(242, 242, 242)\", color: \"rgb(78, 78, 78)\" } : {};\n\n    const navigate = useNavigate();\n    const jumpToDetail = (id) => {\n        navigate(`/product/${id}`)\n    }\n\n    return (\n        <Card className=\"productCard2\"\n            sx={{ maxWidth: 200, maxHeight: 240, border: 1, borderColor: item.isAdded ? 'grey.500' : 'grey.300', borderRadius: 0 }}>\n            {item.isAdded && <Check className=\"product--checked\" size={20} color=\"green\" />}\n            <div onClick={() => jumpToDetail(id)}>\n                <CardMedia\n                    sx={{ height: 110 }}\n                    image={item.img}\n                    title=\"glasses\"\n                />\n                <CardContent sx={{ height: 130 }}>\n                    <Typography variant=\"h6\" component=\"div\" sx={{ fontSize: 13 }}>\n                        {item.name}\n                    </Typography>\n                    <Typography variant=\"body2\" color=\"grey.400\" sx={{ fontStyle: 'italic', fontSize: 12 }}>\n                        {item.brand}\n                    </Typography>\n                    <Typography variant=\"h6\" component=\"div\" sx={{ fontSize: 15 }}>\n                        ${item.price}.00\n                    </Typography>\n                </CardContent>\n            </div>\n            <button className=\"hide\" style={btnStyle} onClick={updateItem}>{btnText}</button>\n        </Card>\n    )\n}","/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/checkout/PaymentForm.jsx",[],[],"/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/account/MyAccount.jsx",[],[],"/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/checkout/Review.jsx",[],[],"/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/hero/Hero.jsx",[],[],"/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/checkout/AddressForm.jsx",[],[],"/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/productCard/ProductCard1.jsx",[],[],"/Users/wtsu/Developer/web-dev-portfolio/salinaka/src/component/store/store.jsx",["157"],[],{"ruleId":"158","severity":1,"message":"159","line":39,"column":6,"nodeType":"160","endLine":39,"endColumn":13,"suggestions":"161"},{"ruleId":"158","severity":1,"message":"162","line":76,"column":6,"nodeType":"160","endLine":76,"endColumn":19,"suggestions":"163"},{"ruleId":"158","severity":1,"message":"164","line":19,"column":8,"nodeType":"160","endLine":19,"endColumn":18,"suggestions":"165"},{"ruleId":"166","severity":1,"message":"167","line":1,"column":8,"nodeType":"168","messageId":"169","endLine":1,"endColumn":13},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'products'. Either i